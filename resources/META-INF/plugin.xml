<idea-plugin version="2">
    <id>com.keshasosiska.easymock-expect-builder</id>
    <name>Easymock expect builder</name>
    <version>1.0.1</version>
    <vendor email="keshasosiska@mail.ru">KeshaSosiska</vendor>

    <description><![CDATA[
        Plugin for intellij idea to create builder for tests from source class.<br>
    ]]></description>

    <change-notes><![CDATA[
      Initial
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="145.0"/>

    <actions>
      <action class="com.keshasosiska.ExpectBuilderAction" id="ExpectBuilderAction" text="Easymock expect builder">
          <add-to-group group-id="GenerateGroup" anchor="last"/>
      </action>
    </actions>
</idea-plugin>